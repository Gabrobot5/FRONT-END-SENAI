algoritmo "correcaomediaponderada"
var
   n1, n2, n3, mediaFinal: real

inicio
// entrada
//3 notas
    leia(n1)
    leia(n2)
    leia(n3)

// processo
//calcular media(anexo)
           mediaFinal <- (n1 * 2 + n2 * 3 + n3 * 5) / 10

// saida
//media do aluno
        escreval("Média final : ", mediaFinal)

fimalgoritmo