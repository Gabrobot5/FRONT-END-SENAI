algoritmo "correcaotabuadado8"

var
   contador : inteiro

inicio
// PROCESSO
//repetição de 10
//(contador >0) e (contador < 10)
            contador <- 1
            enquanto (contador <= 10) faca
            escreval("8 x ", contador, " = ", contador * 8)
            contador <- contador + 1

            fimEnquanto

// SAIDA

fimalgoritmo