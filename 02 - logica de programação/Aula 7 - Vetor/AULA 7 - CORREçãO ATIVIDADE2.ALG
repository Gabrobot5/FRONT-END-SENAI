algoritmo "aula 7 - correção atividade2"
var
   numero : vetor[1..10] de inteiro
   i : inteiro
inicio
      para i <- 1 ate 10 faca
      leia(numero[i])
      fimpara

    //enquanto (condicao) faca ... contador++
    //enquanto (10 >1) faca... i-1
      para i de 10 ate 1 passo -1 faca
      escreval(numero[i])
      fimpara


fimalgoritmo